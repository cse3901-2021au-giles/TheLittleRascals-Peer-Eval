     <div id="add-user" class="none"> 
          <%= form_for @temp_user do |f| %>
               <%= f.label :fname %>
               <%= f.text_field :fname, :required =>  true%>
               <br>
                    <%= f.label :lname %>
               <%= f.text_field :lname, :required =>  true%>
               <br>
               <%= f.label :email %>
               <%= f.text_field :email, :required =>  true%>
               <div class="none">
               <%= f.label :password %>
               <%= f.text_field :password %>
               <br>
               </div>
               <br>
                   <%= f.label :admin %>
                    <%= f.check_box :admin%>
               <br>
               <%= f.submit "Create Account" %>

          <% end %>
     </div>





    <div class="flex">
    <div>
    <h1>Open</h1>
    <% @project.polls.each do |poll|%>
        <%if poll.end_date > Date.today %>
        <div class="flex font20 top-space"> 
        <%= poll.description%>
        <%= poll.created_at%>
        <%= poll.end_date%>
        </div>
        <div class="flex">
        <% @results_dict[poll.id].each do |team|%>
            <div>
                <%= @project.teams.find(team[0]).name%>
                <% team[1].each do |user|%>
                <div class="flex">
                <tr><%= @group.users.find(user[0]).fname %></tr>
                &nbsp
                <div><%= @group.users.find(user[0]).lname %></div>
                &nbsp
                <div><%= user[1] %></div>
                </div>
                <%end %>
            </div>
        <%end%>
        </div>
        <%end%>
    <%end%>
    </div>
    <div>
    <h1>Closed</h1>
        <% @project.polls.each do |poll|%>
        <%if poll.end_date < Date.today %>
        <div class="flex font20 top-space"> 
        <%= poll.description%>
        <%= poll.created_at%>
        <%= poll.end_date%>
        </div>
        <div class="flex">
        <% @results_dict[poll.id].each do |team|%>
            <div>
                <%= @project.teams.find(team[0]).name%>
                <% team[1].each do |user|%>
                <div class="flex">
                <tr><%= @group.users.find(user[0]).fname %></tr>
                &nbsp
                <div><%= @group.users.find(user[0]).lname %></div>
                &nbsp
                <div><%= user[1] %></div>
                </div>
                <%end %>
            </div>
        <%end%>
        </div>
        <%end%>
    <%end%>
    </div>
</div>